// Define a variável idade e atribui o valor 16 a ela
var idade = 16;

// Verifica se a idade é menor que 16
if (idade < 16) {   
    console.log('não vota'); // Se for menor que 16, imprime 'não vota'
} 
// Se a idade não for menor que 16, verifica outras condições
else if (idade >= 16 && idade < 18 || idade > 65) {
    console.log('voto opcional'); // Se a idade estiver entre 16 (inclusive) e menor que 18 (exclusivo) ou for maior que 65, imprime 'voto opcional'
} 
// Se nenhuma das condições anteriores for atendida
else {
    console.log('voto obrigatório'); // Nesse caso, a idade é igual ou maior que 18 e menor ou igual a 65, então imprime 'voto obrigatório'
}
